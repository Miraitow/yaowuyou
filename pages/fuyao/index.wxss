page{
  padding-bottom:280rpx;
}

.set2-text1 {
  font-size: 35rpx;
  color: #303030;
  font-weight: 700;
}
.set-1 {
  margin: 30rpx;
  box-sizing: border-box;
}
.set-2 {
  /* height: 340rpx; */
  border-radius: 10rpx;
  width: 600rpx;
  margin-left: 60rpx;
  margin-top: 60rpx;
  position: relative;
}
.set-view {
  width: 100%;
  background-color: #fff;
  box-shadow: 0px 4px 16px 0px rgba(25, 28, 29, 0.08);
  border-radius: 12rpx;
  margin-top: 15rpx;
  padding: 15rpx;
}
.set-view-text2 {
  font-weight: 700;
  font-size: 35rpx;
}
.set-view-text3 {
  font-size: 28rpx;
  margin-top: 4rpx;
  box-sizing: border-box;
}

.set-2::after {
  content: '';
  /* 必须存在如果没有图标就留空 */
  top: 24%;
  /* 定位 距离*/
  border-style: solid;
  border-left: 1px;
  /* 横线颜色 */
  border-color: #c1e8e0;
  left: -52rpx;
  /* 定位 距离*/
  height: 90%;
  /* 高度 */
  position: absolute;
  /* 定位 */
}
.set-2::before {
  content: "";
  /* 必须存在如果没有图标就留空 */
  /* 微信小程序将本地图片转化成base64方式引用图片 */
  background:url(data:image/png;base64,) no-repeat;
  background-size: 66rpx 66rpx;
  width: 66rpx;
  height: 66rpx;
  position: absolute;
  /* 定位 */
  left: -80rpx;
  /* 移动到左边 */
  font-weight: bold;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  font-family: "iconfont" !important;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  font-size: 54rpx;
  /* 图标大小 */
  font-style: normal;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  -webkit-font-smoothing: antialiased;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  -moz-osx-font-smoothing: grayscale;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
}
.image{
  width: 10vw;
  height: 10vw;
  margin-left: 2vw;
  margin-top: 2vw;
}
.btn {
  color:#fff;
  background-color: #00c399;
  position: absolute;
  right: 3%;
  bottom: 15%;
