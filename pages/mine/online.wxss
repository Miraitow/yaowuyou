
.wrap {
  margin: 0 auto;
  width: 686rpx;
height: 304rpx;
  margin-bottom: 40rpx;
}
.title {
width: 128rpx;
height:44rpx;
font-family: 'PingFang SC';
font-weight: 700;
margin: 10rpx 0 0 10rpx;
font-size: 30rpx;
color: #000000;
}
.serviceCard {
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 686rpx;
height: 240rpx;
background: #FFFFFF;
box-shadow: 0px 0px 10px rgba(205, 205, 205, 0.1);
border-radius: 20rpx;
}
.serviceCard-inner {
  width: 590rpx;
height: 144rpx;
display: flex;
justify-content:flex-start;
align-items: center;
}
.icon {
  width: 96rpx;
  height: 144rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.name {
  width: 96rpx;
  height: 34rpx;
  font-family: 'PingFang SC';
  font-weight: 300;
  font-size: 24rpx;
  text-align: center;
  color: #000000;
}

.bottom-question {
  width: 686rpx;
}

